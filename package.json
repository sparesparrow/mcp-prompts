{
  "name": "mcp-prompts",
  "version": "1.1.0",
  "description": "An MCP server for managing, storing, and providing prompts and prompt templates for LLM interactions",
  "main": "build/index.js",
  "bin": {
    "mcp-prompts": "./bin/cli.js",
    "prompt-cli": "./build/bin/prompt-cli.js"
  },
  "scripts": {
    "build": "node build-tools.js",
    "build:install": "node build-tools.js --install",
    "build:clean": "node build-tools.js --clean",
    "build:force": "node build-tools.js --install --force",
    "build:all": "node build-tools.js --clean --install --force",
    "build:docker": "node build-tools.js --docker",
    "start": "node build/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "ts-node src/tests/promptServer.test.ts",
    "test:ci": "npm run build && node build/tests/promptServer.test.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "node build-tools.js --clean",
    "prepare": "npm run build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "prompt:process": "ts-node bin/prompt-cli.ts process",
    "prompt:process:backup": "ts-node bin/prompt-cli.ts process --backup",
    "prompt:process:keep": "ts-node bin/prompt-cli.ts process --no-cleanup",
    "prompt:tags": "ts-node bin/prompt-cli.ts tags",
    "prompt:tags:list": "ts-node bin/prompt-cli.ts tags list",
    "prompt:tags:add": "ts-node bin/prompt-cli.ts tags add",
    "prompt:tags:remove": "ts-node bin/prompt-cli.ts tags remove",
    "prompt:tags:rename": "ts-node bin/prompt-cli.ts tags rename",
    "prompt:export": "ts-node bin/prompt-cli.ts export",
    "prompt:export:json": "ts-node bin/prompt-cli.ts export --format=json",
    "prompt:export:md": "ts-node bin/prompt-cli.ts export --format=markdown",
    "prompt:import": "ts-node bin/prompt-cli.ts import",
    "prompt:import:dry": "ts-node bin/prompt-cli.ts import --dry-run",
    "prompt:import:force": "ts-node bin/prompt-cli.ts import --force --yes",
    "prompt:organize": "ts-node bin/prompt-cli.ts organize",
    "prompt:organize:dry": "ts-node bin/prompt-cli.ts organize --dry-run",
    "prompt:organize:force": "ts-node bin/prompt-cli.ts organize --force",
    "prompt:pipeline": "ts-node bin/prompt-cli.ts pipeline",
    "prompt:pipeline:dry": "ts-node bin/prompt-cli.ts pipeline --dry-run",
    "prompt:pipeline:verbose": "ts-node bin/prompt-cli.ts pipeline --verbose",
    "prompt:pipeline:keep": "ts-node bin/prompt-cli.ts pipeline --no-cleanup",
    "pgai:migrate": "ts-node scripts/pgai-utils.ts migrate",
    "pgai:migrate:dry": "ts-node scripts/pgai-utils.ts migrate --dry-run",
    "pgai:test": "ts-node scripts/pgai-utils.ts test",
    "pgai:migrate:improved": "ts-node scripts/migrate-prompts.ts",
    "pgai:migrate:improved:dry": "ts-node scripts/migrate-prompts.ts --dry-run",
    "pgai:verify": "ts-node scripts/verify-improved-prompts.ts",
    "install:deps": "npm install pg pgai @types/pg"
  },
  "keywords": [
    "mcp",
    "prompt",
    "template",
    "llm",
    "anthropic",
    "claude",
    "ai",
    "modelcontextprotocol"
  ],
  "author": "MCP Prompts Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.0",
    "@types/pg": "^8.11.11",
    "archiver": "^5.3.2",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "extract-zip": "^2.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.13.3",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.4",
    "@types/commander": "^2.12.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/extract-zip": "^2.0.3",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mcp-prompts.git"
  },
  "files": [
    "build",
    "bin",
    "prompts/examples",
    "prompts/curated",
    "prompts/README.md"
  ]
}
