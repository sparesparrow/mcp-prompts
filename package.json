{
  "name": "@sparesparrow/mcp-prompts",
  "private": true,
  "version": "3.0.6",
  "workspaces": [
    "packages/*",
    "apps/*",
    "docs"
  ],
  "dependencies": {
    "pino": "^9.7.0",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "@sparesparrow/mcp-prompts-adapters-cli": "workspace:*",
    "@sparesparrow/mcp-prompts-adapters-eta": "workspace:*",
    "@sparesparrow/mcp-prompts-adapters-file": "workspace:*",
    "@sparesparrow/mcp-prompts-adapters-mcp": "workspace:*",
    "@sparesparrow/mcp-prompts-adapters-mdc": "workspace:*",
    "@sparesparrow/mcp-prompts-adapters-memory": "workspace:*",
    "@sparesparrow/mcp-prompts-adapters-postgres": "workspace:*",
    "@sparesparrow/mcp-prompts-adapters-rest": "workspace:*",
    "@sparesparrow/mcp-prompts-catalog": "workspace:*",
    "@sparesparrow/mcp-prompts-contracts": "workspace:*",
    "@types/express": "^5.0.3",
    "tsyringe": "^4.10.0",
    "axios": "^1.7.7",
    "handlebars": "^4.7.8",
    "ioredis": "^5.6.1",
    "pg": "^8.11.3",
    "proper-lockfile": "^4.1.2",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@playwright/test": "^1.53.2",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.16.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.0.4",
    "jest-mock-extended": "^4.0.0",
    "playwright": "^1.53.2",
    "prettier": "^3.6.2",
    "puppeteer": "^24.11.2",
    "supertest": "^7.1.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "setup": "bash scripts/setup.sh",
    "dev": "bash scripts/dev.sh",
    "start": "cd apps/server && node dist/index.js",
    "build": "pnpm -r run build",
    "build:watch": "pnpm -r run build:watch",
    "typecheck": "pnpm -r run typecheck",
    "clean": "pnpm -r run clean",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "test": "vitest run",
    "docker:build": "docker build -t mcp-prompts .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f | cat",
    "test:watch": "vitest",
    "docs": "typedoc --out docs/api packages/core/src && markdown-toc -i docs/*.md",
    "prepare": "husky install",
    "release": "standard-version && git push --follow-tags origin main"
  },
  "pnpm": {
    "overrides": {
      "eslint": "^9.0.0"
    }
  }
}
