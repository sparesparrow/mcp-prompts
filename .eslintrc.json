{
  "root": true,
  "env": {
    "es2022": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["dist", "node_modules"],
  "rules": {
    "import/extensions": ["error", "ignorePackages", {
      "ts": "never",
      "js": "never"
    }],
    "no-console": "warn"
  }
}
