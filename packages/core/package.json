{
  "name": "@sparesparrow/mcp-prompts-core",
  "version": "0.1.0",
  "description": "Core domain logic for MCP Prompts Server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js",
      "node": "./dist/index.js"
    },
    "./entities": {
      "import": "./dist/entities/index.js",
      "require": "./dist/entities/index.js",
      "types": "./dist/entities/index.d.ts"
    },
    "./value-objects": {
      "import": "./dist/value-objects/index.js",
      "require": "./dist/value-objects/index.js",
      "types": "./dist/value-objects/index.d.ts"
    },
    "./ports": {
      "import": "./dist/ports/index.js",
      "require": "./dist/ports/index.js",
      "types": "./dist/ports/index.d.ts"
    },
    "./use-cases": {
      "import": "./dist/use-cases/index.js",
      "require": "./dist/use-cases/index.js",
      "types": "./dist/use-cases/index.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas.js",
      "require": "./dist/schemas.js",
      "types": "./dist/schemas.d.ts"
    },
    "./errors": {
      "import": "./dist/errors.js",
      "require": "./dist/errors.js",
      "types": "./dist/errors.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "require": "./dist/config.js",
      "types": "./dist/config.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mcp",
    "prompts",
    "templates",
    "ai",
    "llm",
    "model-context-protocol"
  ],
  "author": "sparesparrow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sparesparrow/mcp-prompts.git"
  },
  "dependencies": {
    "@sparesparrow/mcp-prompts-catalog": "workspace:*",
    "@sparesparrow/mcp-prompts-contracts": "workspace:*",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "handlebars": "^4.7.8",
    "vitest": "^3.2.4",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "swc src -d dist --copy-files && tsc --emitDeclarationOnly --declaration --outDir dist",
    "build:watch": "swc src -d dist --watch --copy-files",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
