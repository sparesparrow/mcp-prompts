{
  "name": "@sparesparrow/mcp-prompts-adapters-file",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "swc src -d dist --copy-files && tsc --emitDeclarationOnly --declaration --outDir dist",
    "build:watch": "swc src -d dist --watch --copy-files",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@sparesparrow/mcp-prompts-core": "workspace:*",
    "@types/proper-lockfile": "^4.1.4",
    "proper-lockfile": "^4.1.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
